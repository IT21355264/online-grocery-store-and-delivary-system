/*------------------------navigation-----------------------------------------------------------------------*/
hr{
	border:1px solid #006600;
}
footer {
	background-color:black;
	padding: 10px;
	text-align: center;
	color: white;

}
.topnav,.bottomnav {
	 background-color:black;
	 overflow: hidden;
}
.img
{
	  display: inline-block;
	  margin-left: 30px;
	  margin-top: 5px;
}
.dropbtn {
	  background-color: black;
	  color: white;
	  padding: 10px;
	  border: none;
	  cursor: pointer;
	   width:120px;  
	  margin-top: 20px;
	  margin-bottom: 0;
	  margin-right: 100px;
	  margin-left: 0;
  
}

.dropdown {
	  right:100%;
	  display: inline-block;
	  float:right;
}

.dropdown-content {
	  display: none;
	  position: absolute;
	  background-color: white;
	  min-width: 160px;
	  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
	  z-index: 1;
	  margin-top: 10px;
	  margin-bottom: 0;
	  margin-right: 10px;
	  margin-left: 10px; 
}
.dropdown-content a {
	  color: black;
	  padding: 12px 16px;
	  text-decoration: none;
	  display: block;
}
.dropdown-content a:hover {background-color: #006600}

.dropdown:hover .dropdown-content { display: block;}

.dropdown:hover .dropbtn {background-color: #006600;}

.nav-button {
	  background-color: black;
	  border: none;
	  color: white;
	  padding: 15px 32px;
	  text-align: center;
	  text-decoration: none;
	  display: inline-block;
	  font-size: 16px;
	  cursor: pointer;
	  margin-right: 35px;
	  margin-left: 100px; 
}
.nav-button:hover {background-color: #006600;}
.nav-button{
    overflow: clip;
}
