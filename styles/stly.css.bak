@import url('https://fonts.googleapis.com/css2?family=Poppins:wght@100;200;300;400;500;600&display=swap');

*{
   font-family: 'Poppins', sans-serif;
   margin:0; padding:0;
   box-sizing: border-box;
   outline: none; border:none;
   text-decoration: none;
}
/*-----------------------------admin viwe--------------------------------------------------*/
.container{
   min-height: 100vh;
   display: flex;
   align-items: center;
   justify-content: center;
   padding:20px;
   padding-bottom: 60px;
   background-image:url("../img/user.jpg");
   background-repeat:no-repeat;
   background-attachment: fixed;  
   background-size: cover;
}

.container .content{
   text-align: center;
}

.container .content h3{
   font-size: 30px;
   color:white;
}

.container .content h3 span{
   background:white;
   color:white;
   border-radius: 5px;
   padding:0 15px;
}

.container .content h1{
   font-size: 50px;
   color:white;
}

.container .content h1 span{
   color:white;
}

.container .content p{
   font-size: 25px;
   margin-bottom: 20px;
}


/*-----------------------------login form--------------------------------------------------*/
.loginbox{
   min-height: 100vh;
   display: flex;
   align-items: center;
   justify-content: right;
   padding:20px;
   padding-bottom: 60px;
   background-image:url("../img/loginback2.jpg");
   background-repeat:no-repeat;
   background-attachment: fixed;  
   background-size: cover;
}

.loginbox form{
   padding:20px;
   border-radius: 5px;
   box-shadow: 0 5px 10px rgba(0,0,0,.1);
   background: #fff;
   text-align: center;
   width: 500px;
    margin-right: 100px;
}

.loginbox form h3{
   font-size: 30px;
   text-transform: uppercase;
   margin-bottom: 10px;
   color:#333;
}

.loginbox form input,
.loginbox form select{
   width: 100%;
   padding:10px 15px;
   font-size: 17px;
   margin:8px 0;
   background: #eee;
   border-radius: 5px;
}

.loginbox form select option{
   background: #fff;
}

.loginbox form .form-btn{
   background: #006600;
   color:white;
   text-transform: capitalize;
   font-size: 20px;
   cursor: pointer;
}

.loginbox form .form-btn:hover{
   background: #006600;
   color:#fff;
}

.loginbox form p{
   margin-top: 10px;
   font-size: 20px;
   color:#333;
}

.loginbox form p a{
   color:#006600;
}

.loginbox form .error-msg{
   margin:10px 0;
   display: block;
   background: #006600;
   color:#fff;
   border-radius: 5px;
   font-size: 20px;
   padding:10px;
}
